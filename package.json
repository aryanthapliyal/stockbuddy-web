{
  "name": "stockbuddy",
  "version": "1.0.0",
  "description": "AI-Powered Stock Prediction Platform",
  "scripts": {
    "install-all": "npm install && cd Backend && npm install && cd ../frontend && npm install",
    "start-backend": "cd Backend && npm run dev",
    "start-model": "cd Model_Backend && python app.py",
    "start-frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-model\" \"npm run start-frontend\"",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "stock-prediction",
    "ai",
    "machine-learning",
    "react",
    "nodejs",
    "python"
  ],
  "author": "StockBuddy Team",
  "license": "MIT"
}
